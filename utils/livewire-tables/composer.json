{
  "name": "getcandy/livewire-tables",
  "type": "project",
  "description": "Livewire Tables",
  "keywords": ["getcandy", "laravel", "livewire", "tables"],
  "license": "MIT",
  "authors": [
    {
      "name": "GetCandy",
      "homepage": "https://getcandy.io/"
    }
  ],
  "require": {
      "php": "^8.0",
      "livewire/livewire": "^2.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5",
      "mockery/mockery": "^1.4.4",
      "orchestra/testbench": "^6.0|^7.0",
      "laravel/pint": "^1.1"
  },
  "autoload": {
      "psr-4": {
          "GetCandy\\LivewireTables\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "GetCandy\\LivewireTables\\Tests\\": "tests/"
      }
  },
  "extra": {
    "getcandy": {
        "name": "Livewire Tables"
    },
    "laravel": {
        "providers": [
            "GetCandy\\LivewireTables\\LivewireTablesServiceProvider"
        ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
